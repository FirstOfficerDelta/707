<!-- 707 Autopilot for Generic FG Autopilot -->
<!-- Retuned and Overhauled -->
<!-- Copyright (c) 2018 Joshua Davidson (it0uchpods) -->

<PropertyList>
	
	<!-- Drivers -->
	
	<filter>
		<name>MAG TRUE ERROR ANGLE</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/orientation/heading-deg</input>
		<reference>/orientation/heading-magnetic-deg</reference>
		<output>/autopilot/internal/mag-true-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>
	
	<filter>
		<name>DRIFT ANGLE</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/orientation/track-magnetic-deg</input>
		<reference>/orientation/heading-magnetic-deg</reference>
		<output>/autopilot/internal/drift-angle-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>
	
	<filter>
		<name>HEADING ERROR DEG</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<and>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>true-heading-hold</value>
					</equals>
					<equals>
						<property>/autopilot/settings/gps-driving-true-heading</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
			<expression>
				<sum>
					<property>/autopilot/route-manager/wp[0]/true-bearing-deg</property>
					<product>
						<property>/autopilot/internal/drift-angle-deg</property>
						<value>-1.0</value>
					</product>
					<product>
						<property>/autopilot/internal/mag-true-error-deg</property>
						<value>-1.0</value>
					</product>
					<product>
						<property>/instrumentation/heading-indicator-fg/indicated-heading-deg</property>
						<value>-1.0</value>
					</product>
				</sum>
			</expression>
		</input>
		<input>
			<condition>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>true-heading-hold</value>
				</equals>
			</condition>
			<expression>
				<sum>
					<property>/autopilot/settings/heading-bug-deg</property>
					<product>
						<property>/autopilot/internal/mag-true-error-deg</property>
						<value>-1.0</value>
					</product>
					<product>
						<property>/instrumentation/heading-indicator-fg/indicated-heading-deg</property>
						<value>-1.0</value>
					</product>
				</sum>
			</expression>
		</input>
		<input>
			<property>/autopilot/settings/heading-bug-deg</property>
			<offset>
				<property>/instrumentation/heading-indicator-fg/indicated-heading-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<output>/autopilot/internal/heading-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>
	
	<filter>
		<name>NAV ERROR DEG</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<sum>
					<property>/instrumentation/nav[0]/radials/target-auto-hdg-deg</property>
					<product>
						<property>/autopilot/internal/mag-true-error-deg</property>
						<value>-1.0</value>
					</product>
					<product>
						<property>/instrumentation/heading-indicator-fg/indicated-heading-deg</property>
						<value>-1.0</value>
					</product>
				</sum>
			</expression>
		</input>
		<output>/autopilot/internal/nav-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>
	
	<filter>
		<name>INTERNAL VERTICAL SPEED COMPUTER</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/instrumentation/gps/indicated-vertical-speed</input>
		<output>/autopilot/internal/vertical-speed-fpm</output>
	</filter>
	
	<filter>
		<name>NAV1 G/S FPM Calc</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/instrumentation/nav[0]/gs-rate-of-climb-fpm</input>
		<output>/autopilot/internal/nav1-rate-of-climb-fpm</output>
		<min>-1200</min>
		<max>0</max>
	</filter>
	
	<!-- Roll Axis -->
	
	<logic>
		<name>ROLL AXIS ENABLED</name>
		<input>
			<or>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>wing-leveler</value>
				</equals>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>dg-heading-hold</value>
				</equals>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>true-heading-hold</value>
				</equals>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>nav1-hold</value>
				</equals>
			</or>
		</input>
		<output>/autopilot/internal/roll-active</output>
	</logic>
	
	<pi-simple-controller>
		<name>ROLL CONTROLLER: HDG/LNAV</name>
		<debug>false</debug>
		<enable>
			<condition>
				<or>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>dg-heading-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>true-heading-hold</value>
					</equals>
				</or>
			</condition>
		</enable>
		<input>/autopilot/internal/heading-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/velocities/mach</property>
						<entry><ind>0.2</ind><dep>-1.0</dep></entry>
						<entry><ind>0.5</ind><dep>-1.6</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ki>-0.000001</Ki>
			<min>
				<property>/autopilot/internal/max-bank</property>
				<scale>-1.0</scale>
			</min>
			<max>/autopilot/internal/max-bank</max>
		</config>
	</pi-simple-controller>
	
	<pi-simple-controller>
		<name>VORLOC TRK</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>nav1-hold</value>
				</equals>
			</condition>
		</enable>
		<input>/autopilot/internal/nav-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<config>
			<Kp>-2.2</Kp>
			<Ki>-0.0001</Ki>
			<min>
				<property>/autopilot/internal/max-bank</property>
				<scale>-1.0</scale>
			</min>
			<max>/autopilot/internal/max-bank</max>
		</config>
	</pi-simple-controller>
	
	<filter>
		<name>ROLL DEG SYNC</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<or>
					<not-equals>
						<property>/autopilot/internal/roll-active</property>
						<value>1</value>
					</not-equals>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>wing-leveler</value>
					</equals>
				</or>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>
			<property>/autopilot/internal/bank-limit</property>
			<scale>-1.0</scale>
		</min>
		<max>/autopilot/internal/bank-limit</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: TARGET ROLL CMD</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<input>
			<condition>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>wing-leveler</value>
				</equals>
			</condition>
			<value>0</value>
		</input>
		<input>/autopilot/internal/target-roll-deg</input>
		<output>/autopilot/internal/target-roll</output>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/autopilot/internal/roll-active</property>
					<value>1</value>
				</equals>
			</condition>
			<value>6</value>
		</max-rate-of-change>
		<max-rate-of-change>50</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Roll Rate</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-0.84</gain>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/internal/roll-active</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<reference>/autopilot/internal/target-roll</reference>
		<output>/autopilot/internal/roll-rate</output>
		<min>-4.8</min>
		<max>4.8</max>
	</filter>
	
	<pid-controller>
		<name>IT-CONTROLLER: ROLL RATE</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/internal/roll-active</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>
			<property>/orientation/roll-rate-degps</property>
		</input>
		<reference>
			<property>/autopilot/internal/roll-rate</property>
		</reference>
		<output>
			<property>/autopilot/internal/aileron-cmd</property>
		</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/velocities/mach</property>
						<entry><ind>0.2</ind><dep>0.15</dep></entry>
						<entry><ind>0.9</ind><dep>0.06</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ti>1.0</Ti>
			<Td>0.0001</Td>
			<u_min>-1.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>IT-CONTROLLER: ROLL CMD</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/internal/roll-active</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>/autopilot/internal/aileron-cmd</input>
		<output>/controls/flight/aileron</output>
		<max-rate-of-change>1.2</max-rate-of-change>
	</filter>
	
	<!-- Pitch Axis -->
	
	<logic>
		<name>VS HOLD ENABLED</name>
		<input>
			<and>
				<or>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>vertical-speed-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>altitude-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>agl-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>gs1-hold</value>
					</equals>
				</or>
				<not-equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-pitch-trim</value>
				</not-equals>
			</and>
		</input>
		<output>/autopilot/internal/vs-active</output>
	</logic>
	
	<filter>
		<name>VERT SPEED FILTER</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/autopilot/settings/vertical-speed-fpm</input>
		<output>/autopilot/internal/vs</output>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>vertical-speed-hold</value>
				</equals>
			</condition>
			<value>300</value>
		</max-rate-of-change>
		<max-rate-of-change>2000</max-rate-of-change>
	</filter>
	
	<filter>
		<name>ALTITUDE CAPTURE/HOLD</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-5</gain>
		<input>
			<expression>
				<sum>
					<property>/instrumentation/altimeter/indicated-altitude-ft</property>
					<value>-2</value>
				</sum>
			</expression>
		</input>
		<reference>/autopilot/settings/target-altitude-ft</reference>
		<output>/autopilot/internal/target-fpm-alt</output>
		<min>-1000</min>
		<max>1000</max>
	</filter>
	
	<filter>
		<name>Gear AGL Altitude FT</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<dif>
					<property>/position/altitude-agl-ft</property>
					<value>6.02</value>
				</dif>
			</expression>
		</input>
		<output>/position/gear-agl-ft-actual</output>
		<min>0</min>
		<max>50000</max>
	</filter>
	
	<filter>
		<name>Gear AGL Altitude M</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<product>
					<property>/position/gear-agl-ft-actual</property>
					<value>0.3048</value>
				</product>
			</expression>
		</input>
		<output>/position/gear-agl-m-actual</output>
		<min>0</min>
		<max>50000</max>
	</filter>
	
	<filter>
		<name>AGL CAPTURE/HOLD</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-5</gain>
		<input>
			<expression>
				<sum>
					<property>/position/gear-agl-ft-actual</property>
					<value>-2</value>
				</sum>
			</expression>
		</input>
		<reference>/autopilot/settings/target-agl-ft</reference>
		<output>/autopilot/internal/target-fpm-agl</output>
		<min>-1000</min>
		<max>1000</max>
	</filter>
	
	<pid-controller>
		<name>FPM HOLD</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/internal/vs-active</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>/autopilot/internal/vertical-speed-fpm</input>
		<reference>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>vertical-speed-hold</value>
				</equals>
			</condition>
			<property>/autopilot/internal/vs</property>
		</reference>
		<reference>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>altitude-hold</value>
				</equals>
			</condition>
			<property>/autopilot/internal/target-fpm-alt</property>
		</reference>
		<reference>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>agl-hold</value>
				</equals>
			</condition>
			<property>/autopilot/internal/target-fpm-agl</property>
		</reference>
		<reference>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>gs1-hold</value>
				</equals>
			</condition>
			<property>/autopilot/internal/nav1-rate-of-climb-fpm</property>
		</reference>
		<output>/autopilot/internal/target-pitch</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/velocities/mach</property>
						<entry><ind>0.2</ind><dep>0.0029</dep></entry>
						<entry><ind>0.9</ind><dep>0.0010</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ti>2.5</Ti>
			<Td>0.0001</Td>
			<u_min>
				<expression>
					<max>
						<dif>
							<property>/orientation/pitch-deg</property>
							<value>5</value>
						</dif>
						<value>-10</value>
					</max>
				</expression>
			</u_min>
			<u_max>
				<expression>
					<min>
						<sum>
							<property>/orientation/pitch-deg</property>
							<value>5</value>
						</sum>
						<value>30</value>
					</min>
				</expression>
			</u_max>
		</config>
	</pid-controller>
	
	<logic>
		<name>PITCH AXIS ENABLED</name>
		<input>
			<and>
				<or>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>vertical-speed-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>pitch-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>aoa-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>altitude-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>agl-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>gs1-hold</value>
					</equals>
				</or>
				<not-equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-pitch-trim</value>
				</not-equals>
			</and>
		</input>
		<output>/autopilot/internal/pitch-active</output>
	</logic>
	
	<filter>
		<name>PITCH DEG SYNC</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<not-equals>
					<property>/autopilot/internal/vs-active</property>
					<value>1</value>
				</not-equals>
			</condition>
		</enable>
		<input>/orientation/pitch-deg</input>
		<output>/autopilot/internal/target-pitch</output>
		<min>-10</min>
		<max>30</max>
	</filter>
	
	<filter>
		<name>AOA PITCH DEG SYNC</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<not-equals>
					<property>/autopilot/locks/altitude</property>
					<value>aoa-hold</value>
				</not-equals>
			</condition>
		</enable>
		<input>/orientation/pitch-deg</input>
		<output>/autopilot/internal/target-pitch-aoa</output>
		<min>-10</min>
		<max>30</max>
	</filter>
	
	<pid-controller>
		<name>AOA HOLD</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>aoa-hold</value>
				</equals>
			</condition>
		</enable>
		<input>/orientation/alpha-deg</input>
		<reference>/autopilot/settings/target-aoa-deg</reference>
		<output>/autopilot/internal/target-pitch-aoa</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/velocities/mach</property>
						<entry><ind>0.2</ind><dep>1.5</dep></entry>
						<entry><ind>0.9</ind><dep>2.0</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ti>0.5</Ti>
			<Td>0.001</Td>
			<u_min>-10</u_min>
			<u_max>30</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>System Command: Pitch Rate</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-0.6</gain>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/internal/pitch-active</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>/orientation/pitch-deg</input>
		<reference>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>aoa-hold</value>
				</equals>
			</condition>
			<property>/autopilot/internal/target-pitch-aoa</property>
		</reference>
		<reference>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>pitch-hold</value>
				</equals>
			</condition>
			<expression>
				<min>
					<max>
						<property>/autopilot/settings/target-pitch-deg</property>
						<value>-10</value>
					</max>
					<value>30</value>
				</min>
			</expression>
		</reference>
		<reference>/autopilot/internal/target-pitch</reference>
		<output>/autopilot/internal/pitch-rate</output>
		<min>-1.5</min>
		<max>1.5</max>
	</filter>
	
	<pid-controller>
		<name>IT-CONTROLLER: PITCH RATE</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/internal/pitch-active</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>
			<property>/orientation/pitch-rate-degps</property>
		</input>
		<reference>
			<property>/autopilot/internal/pitch-rate</property>
		</reference>
		<output>
			<property>/autopilot/internal/elevator-cmd</property>
		</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/velocities/mach</property>
						<entry><ind>0.2</ind><dep>-0.11</dep></entry>
						<entry><ind>0.9</ind><dep>-0.05</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ti>0.5</Ti>
			<Td>0.001</Td>
			<u_min>-1.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>IT-CONTROLLER: PITCH CMD</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/internal/pitch-active</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>/autopilot/internal/elevator-cmd</input>
		<output>/controls/flight/elevator</output>
		<max-rate-of-change>1.2</max-rate-of-change>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: PITCH TRIM</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/autopilot/internal/pitch-active</property>
						<value>1</value>
					</equals>
					<or>
						<greater-than>
							<property>/autopilot/internal/elevator-cmd</property>
							<value>0.01</value>
						</greater-than>
						<less-than>
							<property>/autopilot/internal/elevator-cmd</property>
							<value>-0.01</value>
						</less-than>
					</or>
				</and>
			</condition>
		</enable>
		<input>
			<condition>
				<greater-than>
					<property>/autopilot/internal/elevator-cmd</property>
					<value>0.01</value>
				</greater-than>
			</condition>
			<value>1</value>
		</input>
		<input>
			<condition>
				<less-than>
					<property>/autopilot/internal/elevator-cmd</property>
					<value>-0.01</value>
				</less-than>
			</condition>
			<value>-1</value>
		</input>
		<output>/controls/flight/elevator-trim</output>
		<min>-1.0</min>
		<max>1.0</max>
		<max-rate-of-change>0.01</max-rate-of-change>
	</filter>
	
	<!-- Yaw Axis -->
	
	<filter>
		<name>INTERNAL YAW RATE SENSOR LOW-PASS</name>
		<debug>false</debug>
		<type>exponential</type>
		<input>/orientation/r-body</input>
		<output>/autopilot/internal/yaw-rate-low</output>
		<filter-time>0.02</filter-time>
	</filter>
	
	<filter>
		<name>INTERNAL YAW RATE SENSOR HIGH-PASS</name>
		<debug>false</debug>
		<type>high-pass</type>
		<input>/autopilot/internal/yaw-rate-low</input>
		<output>/autopilot/internal/yaw-rate-high</output>
		<filter-time>0.02</filter-time>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: YAW DAMPER</name>
		<type>gain</type>
		<gain> <!-- RAD2DEG * factor -->
			<expression>
				<table>
					<property>/velocities/mach</property>
					<entry><ind>0.2</ind><dep>57.29580</dep></entry>
					<entry><ind>0.9</ind><dep>11.45916</dep></entry>
				</table>
			</expression>
		</gain>
		<input>/autopilot/internal/yaw-rate-high</input>
		<output>/autopilot/internal/rudder-p</output>
		<min>-0.2</min>
		<max>0.2</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: YAW OUTPUT INTERNAL</name>
		<debug>false</debug>
		<type>noise-spike</type>
		<input>
			<condition>
				<and>
					<equals>
						<property>/autopilot/internal/yaw-damper</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<property>/autopilot/internal/rudder-p</property>
		</input>
		<input>0</input>
		<output>/controls/flight/yaw-damper</output>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/autopilot/internal/yaw-damper</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>0.5</value>
		</max-rate-of-change>
		<max-rate-of-change>10</max-rate-of-change>
	</filter>
	
	<!-- Thrust Axis -->
	
	<pid-controller>
		<name>IAS THR</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-throttle</value>
				</equals>
			</condition>
		</enable>
		<input>/instrumentation/airspeed-indicator/indicated-speed-kt</input>
		<reference>/autopilot/settings/target-speed-kt</reference>
		<output>/controls/engines/throttle-cmd-pid</output>
		<config>
			<Kp>0.06</Kp>
			<beta>1.0</beta>
			<alpha>0.1</alpha>
			<gamma>0.0</gamma>
			<Ti>9.0</Ti>
			<Td>0.00001</Td>
			<u_min>0.05</u_min>
			<u_max>0.95</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>Thrust Filter 1</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-throttle</value>
				</equals>
			</condition>
		</enable>
		<type>noise-spike</type>
		<feedback-if-disabled>false</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/controls/engines/throttle-cmd-pid</input>
		<output>/controls/engines/engine[0]/throttle</output>
		<max-rate-of-change>0.3</max-rate-of-change>
	</filter>
	
	<filter>
		<name>Thrust Filter 2</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-throttle</value>
				</equals>
			</condition>
		</enable>
		<type>noise-spike</type>
		<feedback-if-disabled>false</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/controls/engines/throttle-cmd-pid</input>
		<output>/controls/engines/engine[1]/throttle</output>
		<max-rate-of-change>0.3</max-rate-of-change>
	</filter>
	
	<filter>
		<name>Thrust Filter 3</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-throttle</value>
				</equals>
			</condition>
		</enable>
		<type>noise-spike</type>
		<feedback-if-disabled>false</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/controls/engines/throttle-cmd-pid</input>
		<output>/controls/engines/engine[2]/throttle</output>
		<max-rate-of-change>0.3</max-rate-of-change>
	</filter>
	
	<filter>
		<name>Thrust Filter 4</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-throttle</value>
				</equals>
			</condition>
		</enable>
		<type>noise-spike</type>
		<feedback-if-disabled>false</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/controls/engines/throttle-cmd-pid</input>
		<output>/controls/engines/engine[3]/throttle</output>
		<max-rate-of-change>0.3</max-rate-of-change>
	</filter>
	
</PropertyList>
